{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "secrets:generate": "node scripts/generate-secrets.mjs",
    "env:setup": "node scripts/setup-env.mjs",
    "env:check": "node scripts/check-env.mjs",
    "db:setup": "node scripts/setup-database.mjs",
    "db:verify": "node -e \"import('@prisma/client').then(async ({PrismaClient}) => { const p = new PrismaClient(); try { const m = await p.innerCircleMember.count(); const c = await p.contentMetadata.count(); const t = await p.\\$queryRaw\\`SELECT name FROM sqlite_master WHERE type='table' AND name NOT LIKE 'sqlite_%' ORDER BY name\\`; console.log('✅ Members:', m); console.log('✅ Content:', c); console.log('✅ Tables:', t.length); await p.\\$disconnect(); } catch(e) { console.error('❌ Error:', e.message); } })\"",
    "db:status": "npx prisma db execute --stdin --schema=prisma/schema.prisma -- --command \"SELECT 'Database Status' as check, (SELECT COUNT(*) FROM sqlite_master WHERE type='table') as table_count, (SELECT COUNT(*) FROM inner_circle_members) as member_count, (SELECT COUNT(*) FROM content_metadata) as content_count;\"",
    "dev:init": "npm run env:setup && npm run db:setup && npm run dev",
    "build:check": "npm run env:check && npm run build",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev",
    "dev:content": "contentlayer2 build --watch",
    "dev:full": "concurrently --kill-others-on-fail \"npm run dev:content\" \"npm run dev\"",
    "content:build": "node scripts/build-content.mjs",
    "content:clean": "rimraf .contentlayer",
    "content:validate": "node scripts/validate-content.mjs",
    "content:schema": "node scripts/validate-schema.mjs",
    "content:audit": "npm run content:schema && npm run content:validate",
    "content:full": "npm run content:clean && npm run content:audit && npm run content:build",
    "pdfs": "npx tsx scripts/generate-standalone-pdf.tsx && npx tsx scripts/generate-frameworks-pdf.tsx",
    "pdfs:verify": "npx tsx scripts/verify-assets.ts",
    "pdfs:full": "npm run pdfs && npm run pdfs:verify",
    "postinstall": "prisma generate",
    "prebuild": "npm run env:check && npm run pdfs:full",
    "build": "npm run postinstall && npm run prebuild && cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,json}\"",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:e2e": "playwright test --project=chromium",
    "setup": "npm install && npm run postinstall",
    "clean": "rimraf .next .contentlayer node_modules/.cache",
    "clean:full": "npm run clean && rimraf node_modules && npm cache clean --force",
    "reset": "npm run clean:full && npm run setup",
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next start",
    "analyze": "cross-env ANALYZE=true NODE_OPTIONS=--max-old-space-size=4096 next build",
    "fix:windows": "node scripts/fix-contentlayer-windows.mjs",
    "dev:windows": "cross-env NODE_OPTIONS=--max-old-space-size=4096 DISABLE_CONTENTLAYER=true next dev",
    "build:windows": "cross-env NODE_OPTIONS=--max-old-space-size=4096 DISABLE_CONTENTLAYER=true next build",
    "check:system": "node scripts/check-system.mjs"
  },

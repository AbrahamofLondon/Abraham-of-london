{
  "name": "codex-setup",
  "version": "1.0.0",
  "private": true,
  "engines": { "node": ">=20 <21" },
  "scripts": {
    "prebuild": "node scripts/convert-tailwind-slash-opacity.mjs --check && node scripts/postcss-guard-dryrun.mjs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "prepare": "node scripts/prepare-playwright.mjs && node scripts/setup-husky.mjs",
    "fix:tw-opacity": "node scripts/convert-tailwind-slash-opacity.mjs",
    "check:tw-opacity": "node scripts/convert-tailwind-slash-opacity.mjs --check",
    "guard:css": "node scripts/postcss-guard-dryrun.mjs",
    "fix:css-slash": "cross-env NO_SLASH_OPACITY_MODE=fix node scripts/postcss-guard-dryrun.mjs --write",
    "fix:classnames-slash": "node scripts/codemods/convert-classnames-slash-opacity.mjs",
    "fix:tw-all": "npm run fix:tw-opacity && npm run fix:css-slash && npm run fix:classnames-slash",
    "fix:tw-pr": "npm run fix:tw-all"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "3.0.1",
    "@netlify/functions": "4.2.1",
    "@next/mdx": "14.2.31",
    "@react-email/components": "0.5.0",
    "@react-email/render": "1.2.1",
    "@tailwindcss/typography": "0.5.19",
    "autoprefixer": "10.4.21",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "12.23.12",
    "fuse.js": "7.1.0",
    "gray-matter": "4.0.3",
    "lucide-react": "0.542.0",
    "next": "14.2.31",
    "next-mdx-remote": "4.4.1",
    "next-themes": "0.4.6",
    "nprogress": "0.2.0",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "rehype-stringify": "10.0.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.0",
    "remark-html": "16.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.0",
    "resend": "6.1.2",
    "tailwindcss": "3.4"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "5.0.0",
    "@next/bundle-analyzer": "14.2.32",
    "@playwright/test": "^1.48.2",
    "@types/mdx": "2.0.13",
    "@types/node": "20.14.10",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cross-env": "10.0.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.31",
    "glob": "^11.0.3",
    "husky": "^9.1.6",
    "sharp": "0.34.3",
    "simple-icons": "15.14.0",
    "typescript": "5.9.2"
  }
}

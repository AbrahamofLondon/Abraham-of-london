{
  "name": "codex-setup",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10"
  },
  "scripts": {
    "clean": "rimraf .next .contentlayer",
    "dev": "next dev",
    "prebuild": "npm run audit:images || true && npm run pdfs:generate || true && npm run ci:downloads || true",
    "build": "cross-env NEXT_BUILDER=webpack NEXT_TELEMETRY_DISABLED=1 next build",
    "preprod:build": "npm run fix:bom && npm run downloads:manifest",
    "prod:build": "cross-env NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "start:prod": "next start -p $PORT",
    "lint": "next lint",
    "lint:fix": "next lint --fix || true",
    "typecheck": "tsc --noEmit",
    "audit:assets": "node scripts/audit-assets.mjs",
    "downloads:manifest": "node scripts/build-download-manifest.mjs",
    "orchestrate": "node scripts/orchestrate-fallback.mjs",
    "fix:bom": "node scripts/strip-bom-lf.mjs",
    "content:heal": "node scripts/grand-master.mjs --skip-pdf --skip-deploy --strict=false",
    "pdfs:generate": "node scripts/grand-master.mjs --skip-deploy --strict=false",
    "ci:downloads": "node scripts/run-validate-downloads.mjs",
    "audit:images": "node scripts/audit-images.mjs",
    "analyze": "cross-env ANALYZE=true NEXT_TELEMETRY_DISABLED=1 next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "3.0.1",
    "@netlify/functions": "4.2.1",
    "@react-email/components": "0.5.0",
    "@react-email/render": "1.2.1",
    "@tailwindcss/typography": "0.5.19",
    "autoprefixer": "10.4.21",
    "chokidar": "^3.6.0",
    "clsx": "2.1.1",
    "cross-env": "10.0.0",
    "date-fns": "4.1.0",
    "fast-glob": "^3.3.2",
    "framer-motion": "12.23.12",
    "fuse.js": "7.1.0",
    "get-port": "7.1.0",
    "gray-matter": "4.0.3",
    "lucide-react": "0.542.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-link-attributes": "^4.0.1",
    "next": "^14.2.33",
    "next-mdx-remote": "4.4.1",
    "next-themes": "0.4.6",
    "nprogress": "0.2.0",
    "postcss": "8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.4.0",
    "puppeteer": "22.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "remark": "15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-html": "16.0.1",
    "remark-parse": "11.0.0",
    "resend": "6.1.2",
    "section-matter": "^1.0.0",
    "sharp": "0.34.3",
    "tailwindcss": "3.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@lhci/cli": "^0.15.1",
    "@netlify/plugin-nextjs": "5.14.4",
    "@next/bundle-analyzer": "14.2.32",
    "@playwright/test": "^1.48.2",
    "@types/mdx": "2.0.13",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "esbuild-plugin-tsconfig-paths": "^1.0.1",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.32",
    "glob": "^11.0.3",
    "husky": "^9.1.6",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^3.0.1",
    "remark-rehype": "^11.1.2",
    "simple-icons": "15.14.0",
    "typescript": "^5.9.3"
  }
}
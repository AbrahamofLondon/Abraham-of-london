{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531",
  "engines": {
    "node": ">=20 <22",
    "pnpm": ">=9 <10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "dev:content": "contentlayer2 build --watch",
    "start": "next start",
    "typecheck": "pnpm exec tsc --project tsconfig.json --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "clean:build": "node -e \"const fs=require('fs'); for (const p of ['.next','.contentlayer']) { try { fs.rmSync(p,{recursive:true,force:true}); } catch {} }\"",
    "clean": "node -e \"const fs=require('fs'); const paths=['.next','.contentlayer','.turbo','.reports','node_modules/.cache']; for (const p of paths) { try { fs.rmSync(p,{recursive:true,force:true}); } catch {} } const cwd=process.cwd(); for (const f of fs.readdirSync(cwd)) { if (f.endsWith('.tsbuildinfo') || f === '.tsbuildinfo') { try { fs.rmSync(f,{force:true}); } catch {} } }\"",
    "ts-reset": "node -e \"const fs=require('fs'); for (const p of ['node_modules/.cache','.next']) { try { fs.rmSync(p,{recursive:true,force:true}); } catch {} } const cwd=process.cwd(); for (const f of fs.readdirSync(cwd)) { if (f.endsWith('.tsbuildinfo') || f === '.tsbuildinfo') { try { fs.rmSync(f,{force:true}); } catch {} } }\"",
    "content:build": "contentlayer2 build",
    "content:validate": "node scripts/validate-content.mjs",
    "prepare:content": "pnpm run clean:build && node scripts/fix-contentlayer-issues.mjs && pnpm run content:validate && pnpm run content:build && pnpm run downloads:validate",
    "assets:ensure": "node scripts/ensure-all-assets.mjs",
    "assets:fix-names": "node scripts/fix-asset-names.mjs",
    "pdfs": "node scripts/make-pdfs.mjs",
    "pdfs:force": "rm -rf public/assets/downloads/*.pdf public/assets/resources/pdfs/*.pdf && pnpm run pdfs",
    "downloads:validate": "node scripts/validate-downloads.mjs",
    "prebuild": "node scripts/validate-env.mjs && pnpm run pdfs && pnpm run downloads:validate",
    "build": "pnpm run prepare:content && next build",
    "postbuild": "echo \"âœ“ Build completed successfully\"",
    "setup": "pnpm install && prisma generate && pnpm run prepare:content"
  },
  "dependencies": {
    "@contentlayer2/core": "0.5.8",
    "@contentlayer2/source-files": "0.5.8",
    "@emotion/is-prop-valid": "1.4.0",
    "@heroicons/react": "2.2.0",
    "@netlify/functions": "5.1.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "6.19.1",
    "@react-email/components": "0.0.23",
    "@react-pdf/renderer": "4.3.1",
    "@supabase/supabase-js": "2.87.1",
    "@tailwindcss/typography": "0.5.19",
    "@types/node": "20.11.30",
    "@types/react": "18.3.12",
    "async-mutex": "0.5.0",
    "autoprefixer": "10.4.20",
    "better-sqlite3": "12.5.0",
    "clsx": "2.1.1",
    "contentlayer2": "0.5.8",

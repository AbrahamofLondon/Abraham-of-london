{
  "name": "abraham-of-london",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=9 <10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rimraf .next .turbo node_modules .contentlayer .reports",
    "guard:conflicts": "node -e \"const fs=require('fs'),p=require('path');const walk=d=>fs.readdirSync(d,{withFileTypes:true}).flatMap(e=>e.isDirectory()?walk(p.join(d,e.name)):[p.join(d,e.name)]);const bad=walk('.').filter(f=>/\\.(tsx?|jsx?|mdx?|md)$/.test(f)&&/<<<<<<<|>>>>>>>/.test(fs.readFileSync(f,'utf8')));if(bad.length){console.error('\\nMerge markers found in:\\n'+bad.join('\\n'));process.exit(1);}\"",
    "prebuild": "npm run guard:conflicts && node scripts/prevent-pages-router.js",
    "check": "pnpm run lint && pnpm run typecheck"
  },
  "dependencies": {
    "@heroicons/react": "2.1.5",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "11.2.10",
    "gray-matter": "4.0.3",
    "lucide-react": "0.454.0",
    "next": "14.2.12",
    "next-mdx-remote": "5.0.0",
    "next-themes": "0.4.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "cross-env": "7.0.3",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.12",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3"
  },
  "pnpm": {}
}
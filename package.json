{
  "name": "abraham-of-london",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20 <22",
    "pnpm": ">=9 <10"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "dev:safe": "cross-env NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 next dev",
    "dev:content": "contentlayer2 build --watch",
    "start": "next start",
    "start:safe": "cross-env NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 next start",
    "typecheck": "pnpm exec tsc --project tsconfig.json --noEmit",
    "typecheck:safe": "pnpm exec tsc --project tsconfig.json --noEmit --skipLibCheck",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rimraf .next .turbo .reports .contentlayer node_modules/.cache *.tsbuildinfo",
    "clean:build": "rimraf .next .contentlayer",
    "clean:functions": "rimraf netlify/functions/*.js",
    "fix:prints": "node scripts/fix-print-types.mjs",
    "content:build": "contentlayer2 build",
    "content:dev": "contentlayer2 build --watch",
    "guard:conflicts": "node scripts/check-conflicts.js",
    "build": "pnpm run content:build && next build",
    "build:safe": "cross-env NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 pnpm run content:build && next build",
    "build:windows": "cross-env NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 pnpm run content:build && next build",
    "build:fast": "pnpm run content:build && next build",
    "ci": "pnpm install --frozen-lockfile",
    "build:ci": "pnpm run ci && pnpm run content:build && next build",
    "build:netlify": "CI=false pnpm install --frozen-lockfile --prod=false && pnpm run content:build && cross-env NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 next build",
    "build:netlify:safe": "CI=false pnpm install --frozen-lockfile --prod=false && pnpm run content:build && cross-env NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 next build",
    "check": "pnpm run typecheck",
    "check:safe": "pnpm run typecheck:safe",
    "check:ci": "pnpm run ci && pnpm run check:safe",
    "prebuild": "pnpm run clean:build",
    "postbuild": "echo 'âœ“ Build completed successfully'",
    "postinstall": "contentlayer2 build",
    "assets:scan": "node scripts/scan-content-assets.mjs",
    "pdf:purpose": "tsx scripts/generate-ultimate-purpose-of-man-pdf.tsx",
    "pdfs": "pnpm run pdf:purpose",
    "downloads:validate": "node scripts/validate-downloads.mjs",
    "debug:content": "node scripts/debug-content-routes.mjs",
    "debug:contentlayer": "cross-env CONTENTLAYER_DEBUG=true contentlayer2 build --verbose",
    "debug:content:windows": "set CONTENTLAYER_DEBUG=true && contentlayer2 build --verbose",
    "content:diagnose": "node scripts/diagnose-contentlayer.mjs",
    "content:check": "node scripts/check-contentlayer-output.mjs",
    "content:test": "cross-env CONTENTLAYER_DEBUG=true contentlayer2 build && pnpm run content:check",
    "build:test": "pnpm run clean:build && pnpm run content:test",
    "debug:prints": "node scripts/debug-prints.mjs",
    "content:fix-all": "pnpm run fix:prints && pnpm run content:fix && pnpm run content:validate",
    "test:e2e": "pnpm dlx playwright test",
    "build:functions": "pnpm exec tsc --project tsconfig.functions.json --noEmit",
    "email:test": "tsx scripts/test-email.ts",
    "build:prod": "pnpm run build:safe",
    "install:ci": "pnpm run ci",
    "ts-reset": "pnpm run clean-ts-cache",
    "clean-ts-cache": "rimraf node_modules/.cache .next .tsbuildinfo *.tsbuildinfo",
    "content:fix": "node scripts/fix-contentlayer-issues.mjs",
    "content:validate": "node scripts/validate-content.mjs",
    "gitattributes:validate": "node scripts/validate-gitattributes.js",
    "build:all": "pnpm run content:fix && pnpm run content:validate && pnpm run content:build && next build",
    "prepare": "node -e \"if (process.env.CI !== 'true' && process.env.HUSKY !== '0') { require('child_process').execSync('npx husky install', { stdio: 'inherit' }) } else { console.log('Skipping husky install in CI') }\"",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@contentlayer2/core": "0.5.8",
    "@contentlayer2/source-files": "0.5.8",
    "@emotion/is-prop-valid": "1.4.0",
    "@heroicons/react": "2.2.0",
    "@netlify/functions": "5.1.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "7.1.0",
    "@react-email/components": "0.0.23",
    "@react-pdf/renderer": "4.3.1",
    "@supabase/supabase-js": "2.87.1",
    "@tailwindcss/typography": "0.5.19",
    "@types/node": "20.11.30",
    "@types/react": "18.3.12",
    "@vercel/kv": "3.0.0",
    "async-mutex": "0.5.0",
    "autoprefixer": "10.4.20",
    "better-sqlite3": "12.5.0",
    "clsx": "2.1.1",
    "contentlayer2": "0.5.8",
    "critters": "0.0.25",
    "cross-env": "7.0.3",
    "date-fns": "4.1.0",
    "framer-motion": "11.2.10",
    "fuse.js": "7.0.0",
    "gray-matter": "4.0.3",
    "lucide-react": "0.454.0",
    "next": "14.2.12",
    "next-auth": "4.24.13",
    "next-contentlayer": "0.3.4",
    "next-contentlayer2": "0.5.8",
    "next-mdx-remote": "5.0.0",
    "next-themes": "0.4.3",
    "pg": "8.16.3",
    "pg-hstore": "2.3.4",
    "postcss": "8.4.47",
    "prisma": "7.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "rehype-highlight": "7.0.2",
    "rehype-slug": "6.0.0",
    "remark": "15.0.1",
    "remark-gfm": "4.0.0",
    "remark-html": "16.0.1",
    "resend": "6.5.2",
    "styled-jsx": "5.1.7",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "uuid": "13.0.0",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "5.14.5",
    "@svgr/webpack": "8.1.0",
    "@types/mdx": "2.0.13",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "11.0.0",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.12",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-unused-imports": "4.3.0",
    "glob": "11.0.1",
    "husky": "9.1.7",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "shx": "0.3.4",
    "tsx": "4.20.6"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.24.0",
      "react": "18.3.1",
      "react-dom": "18.3.1"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@opentelemetry/*"
      ],
      "allowedVersions": {
        "react": "18.3.1",
        "react-dom": "18.3.1"
      }
    }
  }
}
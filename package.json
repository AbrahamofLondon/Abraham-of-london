{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "secrets:generate": "node scripts/generate-secrets.mjs",
    "env:setup": "npm run secrets:generate && echo '✅ Environment setup complete'",
    "db:setup": "node scripts/setup-database.mjs",
    "dev:init": "npm run env:setup && npm run db:setup && npm run dev",
    "env:check": "node -e \"import('dotenv/config').then(() => { const required = ['DATABASE_URL', 'INNER_CIRCLE_JWT_SECRET', 'ADMIN_API_KEY']; const missing = required.filter(r => !process.env[r]); if (missing.length) { console.error('❌ Missing:', missing); process.exit(1); } else console.log('✅ All required env vars present'); })\"",
    "build:check": "npm run env:check && npm run build",
    "dev": "next dev",
    "dev:content": "contentlayer2 build --watch",
    "dev:full": "concurrently \"pnpm run dev:content\" \"pnpm run dev\"",
    "content": "pnpm run content:build",
    "content:build": "contentlayer2 build",
    "content:clean": "rimraf .contentlayer",
    "content:validate": "node scripts/validate-content.mjs",
    "content:schema": "node scripts/validate-schema.mjs",
    "content:audit": "pnpm run content:schema && pnpm run content:validate",
    "content:full": "pnpm run content:clean && pnpm run content:audit && pnpm run content:build",
    "pdfs": "npx tsx scripts/generate-standalone-pdf.tsx && npx tsx scripts/generate-frameworks-pdf.tsx",
    "pdfs:verify": "npx tsx scripts/verify-assets.ts",
    "pdfs:full": "pnpm run pdfs && pnpm run pdfs:verify",
    "postinstall": "prisma generate",
    "prebuild": "node scripts/validate-env.mjs && pnpm run content:full && pnpm run pdfs:full",
    "build": "pnpm run postinstall && pnpm run prebuild && next build",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,json}\"",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:e2e": "playwright test --project=chromium",
    "setup": "pnpm install && pnpm run postinstall && pnpm run content:full",
    "clean": "rimraf .next .contentlayer node_modules/.cache",
    "clean:full": "pnpm run clean && rimraf node_modules && pnpm store prune",
    "reset": "pnpm run clean:full && pnpm run setup",
    "start": "next start",
    "analyze": "ANALYZE=true next build"
  },

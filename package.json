{
  "name": "codex-setup",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "ci:downloads": "node scripts/run-validate-downloads.mjs || node -e \"console.log('Skipping download validation (non-fatal).')\"",
    "clean": "node -e \"if (process.platform === 'win32') { require('child_process').execSync('rd /s /q .next .contentlayer 2>nul || echo Cleaned', { stdio: 'inherit' }) } else { require('child_process').execSync('rm -rf .next .contentlayer', { stdio: 'inherit' }) }\"",
    "dev": "npm run contentlayer:clean && contentlayer2 dev && next dev",
    "build": "npm run contentlayer:clean && contentlayer2 build && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "fix:mdx": "node scripts/fix-mdx-imports.mjs",
    "fix:content": "node scripts/fix-content-errors.js",
    "fix:whitespace": "node scripts/fix-whitespace.js",
    "fix:bom": "node scripts/strip-bom-lf.mjs",
    "fix:tw-opacity": "node scripts/convert-tailwind-slash-opacity.mjs",
    "fix:downloads-frontmatter": "node scripts/fix-missing-download-frontmatter.mjs",
    "fix:stray-brackets": "node scripts/remove-stray-angle-brackets.mjs",
    "fix:images": "node scripts/fix-images.mjs",
    "fix:all": "npm run fix:bom && npm run fix:mdx && npm run fix:whitespace && npm run fix:tw-opacity",
    "validate:downloads": "node scripts/validate-downloads.mjs",
    "validate:images": "node scripts/validate-post-images.mjs",
    "audit:downloads": "node scripts/audit-downloads.mjs",
    "audit:images": "node scripts/audit-images.mjs",
    "audit:site": "node scripts/audit-site.mjs",
    "check:mdx": "node scripts/check-mdx-for-html.js",
    "check:exports": "node scripts/check-exports.mjs",
    "check:mdx:syntax": "node scripts/check-mdx-syntax.mjs",
    "check:esbuild": "node scripts/check-esbuild-syntax.mjs",
    "check:json": "node scripts/check-json.mjs",
    "check:env": "tsx scripts/check-env.ts",
    "check:print-slugs": "node scripts/check-print-slugs.mjs",
    "check:unbalanced": "node scripts/find-unbalanced-braces.mjs",
    "downloads:scan": "node scripts/scan-and-fill-downloads.mjs",
    "downloads:fill": "node scripts/scan-and-fill-downloads.mjs --write",
    "downloads:scaffold": "node scripts/scaffold-download-sources.mjs",
    "downloads:normalize": "node scripts/normalize-download-filenames.mjs",
    "downloads:aliases": "node scripts/ensure-download-aliases.mjs",
    "downloads:stubs": "node scripts/generate-download-stubs.mjs",
    "downloads:placeholder": "node scripts/generate-placeholder-downloads.mjs",
    "pdfs:generate": "node scripts/make-pdfs.mjs",
    "pdfs:ensure": "node scripts/ensure-pdfs.mjs",
    "pdfs:render": "node scripts/render-pdfs.mjs",
    "pdfs:print": "node scripts/print-pdfs.mjs",
    "pdfs:all": "node scripts/print-all.mjs",
    "print:serve": "next start -p 5555",
    "print:pdfs": "node scripts/render-pdfs.mjs --base http://localhost:5555 --out public/downloads",
    "covers:generate": "node scripts/generate-covers.mjs",
    "covers:events": "node scripts/covers/make-event-covers.mjs",
    "og:generate": "node scripts/generate-og.mjs",
    "brand:images": "node scripts/brand-images.mjs",
    "signature:make": "node scripts/make-signature.mjs",
    "alias:leaders": "node scripts/alias-leaders-cue-card.mjs",
    "doctor:add": "node scripts/add-doctor-to-package.mjs",
    "tiktok:export": "node scripts/export-tiktok-icon.mjs",
    "sanitize:repo": "node scripts/sanitize-repo.mjs",
    "sanitize:source": "node scripts/sanitize-source.js",
    "orchestrate": "node scripts/orchestrate.mjs",
    "probe:print": "node scripts/probe-print-routes.mjs",
    "check:merge": "node scripts/check-merge-markers.mjs",
    "check:bom": "node scripts/check-bom.mjs",
    "postcss:guard": "node scripts/postcss-guard-dryrun.mjs",
    "playwright:setup": "node scripts/prepare-playwright.mjs",
    "manifest:print": "node scripts/print-manifest.mjs",
    "env:print": "node scripts/print-build-env.js",
    "husky:setup": "node scripts/setup-husky.mjs",
    "prebuild": "npm run fix:mdx",
    "build:full": "npm run fix:all && npm run contentlayer:clean && contentlayer2 build && next build",
    "prod:build": "npm run fix:all && npm run contentlayer:clean && cross-env NODE_ENV=production next build",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "echo \"No tests configured yet. Skipping.\" && exit 0",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "format": "prettier --write .",
    "contentlayer:clean": "node -e \"if (process.platform === 'win32') { console.log('ðŸ§¹ Windows: Clearing Contentlayer cache...'); require('child_process').execSync('rd /s /q .contentlayer 2>nul || echo Cache already cleared', { stdio: 'inherit' }); } else { console.log('ðŸ§¹ Clearing Contentlayer cache...'); require('child_process').execSync('rm -rf .contentlayer', { stdio: 'inherit' }); }\"",
    "contentlayer:generate": "npm run contentlayer:clean && contentlayer2 build",
    "contentlayer:build": "contentlayer2 build",
    "contentlayer:dev": "contentlayer2 dev",
    "contentlayer:watch": "contentlayer2 build --watch",
    "windows:doctor": "node scripts/windows-doctor.mjs"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@netlify/functions": "4.2.1",
    "@react-email/components": "0.5.0",
    "@tailwindcss/typography": "0.5.19",
    "autoprefixer": "10.4.21",
    "clsx": "2.1.1",
    "cross-env": "10.0.0",
    "date-fns": "4.1.0",
    "framer-motion": "12.23.12",
    "gray-matter": "4.0.3",
    "lucide-react": "0.542.0",
    "next": "14.2.33",
    "next-mdx-remote": "4.4.1",
    "next-themes": "0.4.6",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.1",
    "resend": "6.1.2",
    "rimraf": "5.0.5",
    "sharp": "0.34.3",
    "tailwindcss": "3.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.0",
    "@next/mdx": "14.2.33",
    "@playwright/test": "1.48.2",
    "@types/node": "20.14.10",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "contentlayer2": "0.5.8",
    "next-contentlayer2": "0.5.8",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.33",
    "husky": "9.1.6",
    "prettier": "3.3.3",
    "puppeteer": "22.1.0",
    "tsx": "4.7.0",
    "typescript": "5.5.4"
  },
  "pnpm": {
    "overrides": {
      "@effect-ts/system": "0.57.5",
      "@effect-ts/core": "0.57.5"
    }
  }
}
{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail --names \"CONTENT,DEV\" \"contentlayer2 dev\" \"next dev\"",
    "build": "contentlayer2 build && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,json,css,scss,yml,yaml}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next .contentlayer",
    "clean:win": "powershell -Command \"if (Test-Path .next) { Remove-Item .next -Recurse -Force }; if (Test-Path .contentlayer) { Remove-Item .contentlayer -Recurse -Force }\"",
    "reset": "pnpm clean:win && pnpm install",
    "content:build": "contentlayer2 build",
    "content:watch": "contentlayer2 dev",
    "db:setup": "powershell -Command \"if (-Not (Test-Path 'prisma')) { New-Item -ItemType Directory -Path 'prisma' }; New-Item -ItemType File -Path 'prisma/dev.db' -Force\" && prisma generate && prisma db push --url=\"file:./prisma/dev.db\" --accept-data-loss",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio --url=\"file:./prisma/dev.db\"",
    "test": "jest --passWithNoTests",
    "build:windows": "pnpm clean:win && pnpm build"
  },
  "dependencies": {
    "@contentlayer/source-files": "0.3.4",
    "@heroicons/react": "2.2.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.10.2",
    "@react-email/components": "0.0.23",
    "@react-pdf/renderer": "4.3.1",
    "@tailwindcss/typography": "0.5.19",
    "autoprefixer": "10.4.20",
    "better-sqlite3": "12.5.0",
    "clsx": "2.1.1",
    "cookie": "1.1.1",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "framer-motion": "11.2.10",
    "jsonwebtoken": "9.0.3",
    "lucide-react": "0.454.0",
    "next": "14.2.12",
    "next-auth": "4.24.13",
    "next-mdx-remote": "5.0.0",
    "pdf-lib": "1.17.1",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "recharts": "3.6.0",
    "redis": "5.10.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "3.0.0",
    "rehype-highlight": "7.0.2",
    "rehype-slug": "5.1.0",
    "remark": "14.0.3",
    "remark-gfm": "3.0.1",
    "resend": "6.5.2",
    "sharp": "0.33.5",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "3.4.14",
    "uuid": "13.0.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@contentlayer2/source-files": "0.5.8",
    "@next/bundle-analyzer": "14.2.12",
    "@types/node": "20.11.30",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@typescript-eslint/parser": "8.50.1",
    "concurrently": "8.2.2",
    "contentlayer2": "0.5.8",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.12",
    "eslint-config-prettier": "9.1.2",
    "gray-matter": "4.0.3",
    "jest": "30.0.0",
    "next-contentlayer2": "0.5.8",
    "prettier": "3.3.3",
    "prisma": "5.10.2",
    "rimraf": "6.0.1",
    "tsx": "4.20.6",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@9.15.9",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
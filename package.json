{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --webpack",
    "dev:full": "pnpm env:check && concurrently --kill-others-on-fail=false --names \"CONTENT,DEV\" \"pnpm contentlayer:watch\" \"next dev --webpack\"",
    "dev:windows": "cross-env IS_WINDOWS=true next dev --webpack",
    "dev:profile": "cross-env NEXT_PROFILING=true next dev --webpack",
    "start": "next start",
    "preview": "next start --port 3001",
    "prebuild": "pnpm vault:audit",
    "build": "pnpm build:downloads && node scripts/build-manifest-check.mjs && node scripts/build-manifest.mjs && cross-env NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next build --webpack",
    "build:ci": "pnpm prebuild:ci && pnpm build",
    "build:netlify": "pnpm build:ci",
    "build:safe": "pnpm vault:fix && pnpm fix:encoding && pnpm content:validate && pnpm build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prebuild:ci": "pnpm vault:fix && pnpm fix:yaml && pnpm fix:windows && pnpm vault:ready",
    "build:pdfs:clean": "node -e \"console.log('build:pdfs:clean disabled (registry is committed)')\"",
    "build:pdfs": "node -e \"console.log('build:pdfs disabled (run pnpm pdf:build:full manually if needed)')\"",
    "build:downloads": "tsx scripts/refresh-downloads.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force && pnpm db:seed",
    "db:status": "prisma migrate status",
    "postinstall": "prisma generate",
    "portfolio:sync": "pnpm db:generate && pnpm db:push && pnpm db:seed",
    "content:clean": "rimraf .contentlayer .next/content",
    "contentlayer:build": "contentlayer2 build",
    "contentlayer:safe": "contentlayer2 build --clearCache",
    "contentlayer:watch": "contentlayer2 watch",
    "content:validate": "node scripts/validate-frontmatter.mjs",
    "vault:audit": "node scripts/institutional-audit.mjs",
    "vault:sync": "node scripts/vault-sync.mjs",
    "vault:fix": "node scripts/vault-repair.mjs",
    "vault:glossary": "node scripts/glossary-injector.mjs",
    "vault:check": "node scripts/build-manifest.mjs && node scripts/validate-links.mjs",
    "vault:manifest": "node scripts/generate-vault-manifest.mjs",
    "vault:deploy": "pnpm vault:audit && pnpm vault:sync && next build",
    "vault:ready": "pnpm contentlayer:safe && pnpm vault:audit && pnpm vault:glossary && pnpm vault:check && pnpm vault:manifest",
    "vault:dry": "node scripts/vault-repair-dry.mjs",
    "assets:optimize": "node scripts/optimize-assets.js",
    "assets:enterprise": "tsx scripts/enterprise-asset-optimizer.ts",
    "assets:enterprise:production": "cross-env NODE_ENV=production tsx scripts/enterprise-asset-optimizer.ts --quality=premium",
    "assets:audit": "node scripts/audit-assets.js",
    "assets:metrics": "node scripts/analyze-metrics.js",
    "assets:clean": "rimraf public/optimized-* public/fonts/optimized logs metrics .cache/assets",
    "assets:images:optimize": "node scripts/optimize-images.js",
    "optimize:premium": "tsx scripts/enterprise-asset-optimizer.ts --images-only --avif --ultra-quality",
    "assets:fonts:generate": "node scripts/generate-fonts.cjs",
    "pdf:build:full": "pnpm pdf:registry:full && pnpm pdf:generate-all && pnpm pdf:sync",
    "pdf:generate-all": "tsx scripts/pdf/unified-pdf-generator.ts --tier all --scan-content --use-puppeteer --use-universal --validate --no-empty",
    "pdf:registry:full": "pnpm pdf:registry:build && pnpm pdf:registry && pnpm pdf:variants",
    "pdf:registry:build": "tsx scripts/pdf/build-pdf-registry-generated.ts",
    "pdf:sync": "tsx scripts/pdf/unified-pdf-generator.ts --scan-content",
    "pdf:audit": "pnpm pdf:list && pnpm pdf:validate",
    "pdf:repair": "tsx scripts/pdf/repair-pdfs.ts --repair-empty --regenerate-corrupt",
    "health": "tsx scripts/system/check-system-health.ts",
    "check:all": "pnpm health && pnpm check:system && pnpm check:security && pnpm check:content-boundary",
    "check:system": "tsx scripts/system/check-system.ts",
    "check:security": "pnpm exec audit-ci --config .auditci.json",
    "security:audit": "pnpm audit --audit-level=high",
    "fix:all": "pnpm fix:prisma && pnpm fix:imports && pnpm fix:build && pnpm fix:windows && pnpm fix:yaml && pnpm fix:unsafe",
    "fix:windows": "node scripts/windows-fix.js",
    "fix:yaml": "node scripts/fix-yaml-specific.cjs",
    "fix:mdx": "node scripts/fix-mdx-encoding.mjs",
    "mdx:sanitize": "node scripts/mdx/sanitize-all-mdx.mjs",
    "fix:unsafe": "node scripts/fix-unsafe-strings-surgical.cjs",
    "env:check": "tsx scripts/environment/check-env.ts || echo 'Environment check completed'",
    "clean:hard": "node scripts/clean-project.js",
    "verify:production": "cross-env TARGET_URL=https://www.abrahamoflondon.org node scripts/system/verify-deployment.mjs",
    "verify:local": "cross-env TARGET_URL=http://localhost:3000 node scripts/system/verify-deployment.mjs",
    "test": "node scripts/validate-links.mjs",
    "deploy": "node scripts/validate-links.mjs && git add . && git commit -m 'System Sync' && git push"
  },

{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "dev": "next dev",
    "dev:full": "pnpm env:check && concurrently --kill-others-on-fail=false --names \"CONTENT,DEV\" \"pnpm contentlayer:watch\" \"next dev\"",
    "dev:windows": "cross-env IS_WINDOWS=true next dev",
    "dev:profile": "cross-env NEXT_PROFILING=true next dev",
    "prebuild": "pnpm fm:validate && pnpm fix:windows && pnpm fix:yaml && pnpm contentlayer:safe || echo 'Contentlayer build warning (continuing anyway)'",
    "build": "cross-env TURBOPACK_DISABLED=true NEXT_RUNTIME=nodejs next build",
    "build:fast": "cross-env TURBOPACK_DISABLED=false next build",
    "build:netlify": "pnpm install --frozen-lockfile && pnpm content:full && pnpm build",
    "build:analyze": "cross-env ANALYZE=true NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "preview": "next start --port 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next .contentlayer .turbo .cache node_modules/.cache",
    "clean:hard": "rimraf node_modules .next .contentlayer .turbo .cache node_modules/.cache",
    "env:setup": "tsx scripts/environment/setup-env.ts",
    "env:check": "tsx scripts/environment/check-env.ts || echo 'Environment check completed (some warnings may appear)'",
    "env:validate": "tsx scripts/environment/validate-env.ts",
    "env:production:sync": "cross-env NODE_ENV=production tsx scripts/environment/generate-production-env.ts",
    "env:staging:sync": "cross-env NODE_ENV=staging tsx scripts/environment/generate-staging-env.ts",
    "env:development:sync": "cross-env NODE_ENV=development tsx scripts/environment/generate-development-env.ts",
    "prisma:generate": "pnpm exec prisma generate --schema=./prisma/schema.prisma",
    "prisma:push": "pnpm exec prisma db push --accept-data-loss --schema=./prisma/schema.prisma",
    "prisma:studio": "pnpm exec prisma studio --url=\"file:./prisma/dev.db\" --schema=./prisma/schema.prisma",
    "db:setup": "tsx scripts/database/setup-database.ts",
    "db:seed": "tsx scripts/database/seed-database.ts",
    "db:verify": "tsx scripts/database/verify-database.ts",
    "db:backup": "tsx scripts/database/backup-database.ts",
    "db:health": "tsx scripts/database/check-database-health.ts",
    "contentlayer": "pnpm exec contentlayer2",
    "contentlayer:watch": "pnpm exec contentlayer2 dev",
    "contentlayer:build": "pnpm exec contentlayer2 build",
    "contentlayer:safe": "pnpm exec contentlayer2 build --clearCache",
    "fm:validate": "node scripts/validate-frontmatter.mjs",
    "fix:yaml": "node scripts/fix-yaml-specific.js",
    "content:clean": "rimraf .contentlayer .next/content",
    "content:validate": "node scripts/validate-frontmatter.mjs",
    "content:schema": "tsx scripts/content/validate-schema.ts",
    "content:stats": "tsx scripts/content/content-statistics.ts",
    "content:backup": "tsx scripts/content/backup-content.ts",
    "content:optimize": "tsx scripts/content/optimize-content.ts",
    "content:full": "pnpm content:clean && pnpm fix:yaml && pnpm contentlayer:safe && pnpm content:validate",
    "pdfs:generate": "tsx scripts/pdf/unified-pdf-generator.ts",
    "pdfs:cycle": "tsx scripts/pdf/unified-pdf-generator.ts --tier all",
    "pdfs:sync": "tsx scripts/pdf/sync-pdfs.ts",
    "pdfs:validate": "tsx scripts/pdf/validate-pdfs.ts",
    "pdfs:list": "tsx scripts/pdf/list-pdfs.ts",
    "pdfs:cleanup": "tsx scripts/pdf/cleanup-pdfs.ts",
    "pdfs:optimize": "tsx scripts/pdf/optimize-pdfs.ts",
    "pdfs:build": "pnpm pdfs:cycle && pnpm pdfs:sync",
    "pdfs:audit": "pnpm pdfs:list && pnpm pdfs:validate",
    "assets:fonts:optimize": "node scripts/optimize-fonts.js",
    "assets:fonts:generate": "node scripts/generate-fonts.js",
    "assets:images:optimize": "node scripts/optimize-images.js",
    "assets:images:resize": "node scripts/resize-images.js",
    "assets:audit": "node scripts/audit-assets.js",
    "assets:optimize": "pnpm assets:fonts:optimize && pnpm assets:images:optimize",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "tsx scripts/security/security-scan.ts",
    "security:full": "pnpm security:scan && pnpm security:audit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\"\\.(test|spec)\\.(ts|tsx)$\" --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test --project=chromium",
    "health": "tsx scripts/system/check-system-health.ts",
    "check:system": "tsx scripts/system/check-system.ts",
    "check:dependencies": "tsx scripts/system/check-dependencies.ts",
    "check:security": "audit-ci --config .auditci.json",
    "check:all": "pnpm check:system && pnpm check:dependencies && pnpm check:security",
    "fix:windows": "node scripts/windows-fix.js",
    "fix:imports": "tsx scripts/system/fix-imports.ts",
    "fix:build": "tsx scripts/system/fix-build-issues.ts",
    "fix:prisma": "tsx scripts/system/fix-prisma-working.ts",
    "fix:all": "pnpm fix:prisma && pnpm fix:imports && pnpm fix:build",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "sitemap:validate": "node scripts/validate-sitemap.js",
    "seo:audit": "node scripts/seo-audit.js",
    "links:check": "node scripts/check-links.js",
    "audit:local": "node scripts/check-links.js --local",
    "audit:staging": "node scripts/check-links.js --url=https://staging.abrahamoflondon.org",
    "audit:production": "node scripts/check-links.js --url=https://www.abrahamoflondon.org",
    "postinstall": "pnpm prisma:generate",
    "postbuild": "pnpm sitemap:generate"
  },

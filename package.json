{
  "name": "abraham-of-london",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "env:setup": "npx tsx scripts/environment/setup-env.ts",
    "env:check": "npx tsx scripts/environment/check-env.ts",
    "env:validate": "npx tsx scripts/environment/validate-env.ts",
    "env:generate": "npx tsx scripts/environment/generate-env.ts",
    "env:production": "npx tsx scripts/environment/generate-production-env.ts",
    "env:staging": "npx tsx scripts/environment/generate-staging-env.ts",
    "env:development": "npx tsx scripts/environment/generate-development-env.ts",
    "prisma:clean": "rimraf node_modules/.prisma",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --config=prisma/config.ts --name init",
    "prisma:migrate:deploy": "npx prisma migrate deploy --config=prisma/config.ts",
    "prisma:push": "npx prisma db push --config=prisma/config.ts",
    "prisma:studio": "npx prisma studio --config=prisma/config.ts",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate",
    "db:reset": "npx prisma migrate reset --config=prisma/config.ts --force",
    "db:seed": "npx tsx scripts/database/seed-database.ts",
    "db:seed:production": "npx tsx scripts/database/seed-production.ts",
    "db:seed:test": "npx tsx scripts/database/seed-test.ts",
    "db:verify": "npx tsx scripts/database/verify-database.ts",
    "db:status": "npx tsx scripts/database/check-database-status.ts",
    "db:backup": "npx tsx scripts/database/backup-database.ts",
    "db:restore": "npx tsx scripts/database/restore-database.ts",
    "db:optimize": "npx tsx scripts/database/optimize-database.ts",
    "db:cleanup": "npx tsx scripts/database/cleanup-database.ts",
    "db:monitor": "npx tsx scripts/database/monitor-database.ts",
    "db:health": "npx tsx scripts/database/check-database-health.ts",
    "neon:setup": "npx tsx scripts/database/setup-neon-tables.ts",
    "neon:verify": "npx tsx scripts/database/verify-neon.ts",
    "neon:test": "npx tsx scripts/database/test-neon-connection.ts",
    "secrets:generate": "npx tsx scripts/secrets/generate-secrets.ts",
    "secrets:validate": "npx tsx scripts/secrets/validate-secrets.ts",
    "secrets:rotate": "npx tsx scripts/secrets/rotate-secrets.ts",
    "contentlayer": "contentlayer build",
    "dev:with-contentlayer": "contentlayer build --watch & next dev",
    "build:with-contentlayer": "contentlayer build && next build",
    "content:build": "npx tsx scripts/content/build-content.ts",
    "content:clean": "rimraf .contentlayer .next/content",
    "content:watch": "contentlayer2 build --watch",
    "content:validate": "npx tsx scripts/content/validate-content.ts",
    "content:schema": "npx tsx scripts/content/validate-schema.ts",
    "content:audit": "npm run content:schema && npm run content:validate",
    "content:full": "npm run content:clean && npm run content:audit && npm run content:build",
    "content:stats": "npx tsx scripts/content/content-statistics.ts",
    "content:backup": "npx tsx scripts/content/backup-content.ts",
    "content:restore": "npx tsx scripts/content/restore-content.ts",
    "content:optimize": "npx tsx scripts/content/optimize-content.ts",
    "content:index": "npx tsx scripts/content/index-content.ts",
    "pdfs": "npx tsx scripts/pdf/generate-pdfs.ts",
    "pdfs:generate": "npx tsx scripts/pdf/generate-all-pdfs.ts",
    "pdfs:single": "npx tsx scripts/pdf/generate-single-pdf.ts",
    "pdfs:batch": "npx tsx scripts/pdf/generate-batch-pdfs.ts",
    "pdfs:type": "npx tsx scripts/pdf/generate-by-type.ts",
    "pdfs:legacy": "npx tsx scripts/pdf/generate-legacy-canvas.ts",
    "pdfs:canvas": "npm run pdfs:legacy",
    "pdfs:standalone": "npx tsx scripts/pdf/generate-standalone-pdf.ts",
    "pdfs:frameworks": "npx tsx scripts/pdf/generate-frameworks-pdf.ts",
    "pdfs:list": "npx tsx scripts/pdf/pdf-registry.ts --list",
    "pdfs:scan": "npx tsx scripts/pdf/pdf-registry.ts --scan",
    "pdfs:missing": "npx tsx scripts/pdf/generate-missing-pdfs.ts",
    "pdfs:validate": "npx tsx scripts/pdf/validate-pdfs.ts",
    "pdfs:audit": "npm run pdfs:list && npm run pdfs:scan",
    "pdfs:preview": "npx tsx scripts/pdf/pdf-registry.ts --preview",
    "pdfs:status": "npx tsx scripts/pdf/pdf-registry.ts --status",
    "pdfs:verify": "npx tsx scripts/pdf/verify-assets.ts",
    "pdfs:sync": "npx tsx scripts/pdf/sync-pdfs.ts",
    "pdfs:cleanup": "npx tsx scripts/pdf/cleanup-pdfs.ts",
    "pdfs:optimize": "npx tsx scripts/pdf/optimize-pdfs.ts",
    "pdfs:compress": "npx tsx scripts/pdf/compress-pdfs.ts",
    "pdfs:watermark": "npx tsx scripts/pdf/add-watermarks.ts",
    "pdfs:metadata": "npx tsx scripts/pdf/update-metadata.ts",
    "pdfs:security": "npx tsx scripts/pdf/secure-pdfs.ts",
    "pdfs:watch": "npx tsx scripts/pdf/watch-pdfs.ts",
    "pdfs:monitor": "npx tsx scripts/pdf/monitor-pdfs.ts",
    "dev": "cross-env NODE_ENV=development next dev",
    "dev:content": "npm run content:watch",
    "dev:full": "concurrently --kill-others-on-fail --names \"CONTENT,DEV\" \"npm run content:watch\" \"npm run dev\"",
    "dev:with-pdfs": "npm run pdfs:generate && npm run dev",
    "dev:monitor": "concurrently --kill-others-on-fail --names \"CONTENT,DEV,PDF\" \"npm run content:watch\" \"npm run dev\" \"npm run pdfs:watch\"",
    "dev:init": "npm run env:setup && npm run db:setup && npm run db:seed && npm run dev",
    "dev:init:full": "npm run clean:full && npm install && npm run env:setup && npm run db:setup && npm run db:seed && npm run dev",
    "prebuild": "npm run prisma:generate && npm run env:check && npm run content:full",
    "prebuild:production": "npm run prisma:generate && npm run env:production && npm run content:full",
    "build": "npm run prebuild && cross-env NODE_ENV=production next build",
    "build:production": "npm run prebuild:production && cross-env NODE_ENV=production next build",
    "build:staging": "npm run env:staging && npm run content:full && cross-env NODE_ENV=staging next build",
    "build:check": "npm run env:check && npm run content:full && npm run build",
    "build:netlify": "cross-env NETLIFY=true NODE_ENV=production npm run build",
    "build:windows": "cross-env IS_WINDOWS=true NODE_ENV=production npm run build",
    "build:windows:netlify": "cross-env IS_WINDOWS=true NETLIFY=true NODE_ENV=production npm run build",
    "start": "cross-env NODE_ENV=production next start",
    "start:production": "npm run start",
    "start:staging": "cross-env NODE_ENV=staging next start",
    "start:netlify": "cross-env NETLIFY=true NODE_ENV=production next start",
    "clean": "rimraf .next .contentlayer .cache node_modules/.cache .turbo",
    "clean:build": "rimraf .next .cache .turbo",
    "clean:logs": "rimraf logs/*.log",
    "clean:temp": "rimraf temp/*",
    "clean:full": "npm run clean && rimraf node_modules package-lock.json yarn.lock pnpm-lock.yaml && npm cache clean --force",
    "clean:all": "npm run clean:full && npm run clean:logs && npm run clean:temp",
    "clean:duplicates": "npx tsx scripts/content/clean-duplicate-files.ts",
    "setup": "pnpm install",
    "setup:full": "npm run clean:full && pnpm install",
    "reset": "npm run clean:all && npm run setup:full",
    "reset:hard": "git clean -fdx && npm run setup:full",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --max-warnings=0",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx --fix --max-warnings=0 --report-unused-disable-directives",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,json,css,scss,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,json,css,scss,yml,yaml}\"",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:strict": "tsc --noEmit --project tsconfig.json --strict --noImplicitAny --noImplicitThis --alwaysStrict --strictBindCallApply --strictFunctionTypes --strictNullChecks --strictPropertyInitialization --useUnknownInCatchVariables",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=\"\\.(test|spec)\\.(ts|tsx)$\" --passWithNoTests",
    "test:integration": "jest --testPathPattern=\"\\.(test|spec)\\.(ts|tsx)$\" --testNamePattern=\"integration\"",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:all": "playwright test",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:ci": "npm run test:unit -- --ci --maxWorkers=2",
    "fix:prisma": "npx tsx scripts/fix-prisma-working.ts",
    "fix:contentlayer": "npx tsx scripts/system/fix-contentlayer-issues.ts",
    "fix:windows": "npx tsx scripts/system/fix-contentlayer-windows.ts",
    "fix:windows:all": "npx tsx scripts/system/fix-windows-issues.ts",
    "fix:imports": "npx tsx scripts/system/fix-imports.ts",
    "fix:scripts": "node scripts/fix-hashbang.js",
    "fix:imports:all": "npx tsx scripts/system/fix-all-imports.ts",
    "fix:build": "npx tsx scripts/fix-build-issues.ts",
    "fix:ts-errors": "npx tsx scripts/system/fix-typescript-errors.ts",
    "fix:ts-errors:all": "npx tsx scripts/system/fix-all-typescript-errors.ts",
    "fix:all": "npm run fix:prisma && npm run fix:contentlayer && npm run fix:ts-errors:all && npm run fix:imports:all",
    "dev:windows": "cross-env NODE_ENV=development IS_WINDOWS=true next dev",
    "build:windows:production": "cross-env NODE_ENV=production IS_WINDOWS=true next build",
    "check:system": "npx tsx scripts/system/check-system.ts",
    "check:dependencies": "npx tsx scripts/system/check-dependencies.ts",
    "check:security": "npx audit-ci --config .auditci.json",
    "check:outdated": "pnpm outdated || true",
    "check:vulnerabilities": "pnpm audit || true",
    "check:all": "npm run check:system && npm run check:dependencies && npm run check:security && npm run check:outdated && npm run check:vulnerabilities",
    "standardize:extensions": "npx tsx scripts/system/standardize-extensions.ts",
    "standardize:check": "npx tsx scripts/system/standardize-extensions.ts --dry-run",
    "standardize:all": "npm run standardize:extensions && npm run fix:imports && npm run lint && npm run format",
    "auto:resolve": "npx tsx scripts/system/auto-resolve-conflicts.ts",
    "auto:fix": "npm run fix:all && npm run lint && npm run format",
    "health": "npx tsx scripts/system/check-system-health.ts",
    "health:full": "npm run health && npm run db:health && npm run content:stats",
    "monitor": "npx tsx scripts/system/monitor-system.ts",
    "monitor:performance": "npx tsx scripts/system/monitor-performance.ts",
    "monitor:memory": "npx tsx scripts/system/monitor-memory.ts",
    "backup": "npm run db:backup && npm run content:backup",
    "restore": "npm run db:restore && npm run content:restore",
    "maintenance": "npm run db:optimize && npm run content:optimize && npm run pdfs:optimize",
    "maintenance:full": "npm run maintenance && npm run clean:temp && npm run clean:logs",
    "api:generate-pdf": "npx tsx scripts/api/test-pdf-generation.ts",
    "api:generate-all": "npx tsx scripts/api/test-bulk-generation.ts",
    "api:test": "npx tsx scripts/api/test-api-endpoints.ts",
    "api:health": "npx tsx scripts/api/test-health-endpoint.ts",
    "test:neon": "npx tsx scripts/database/test-neon-connection.ts",
    "deploy:prepare": "npm run env:production && npm run type-check:strict && npm run test:ci && npm run build:production",
    "deploy:staging": "npm run env:staging && npm run type-check && npm run test:ci && npm run build:staging",
    "deploy:netlify": "npm run type-check && npm run lint && npm run build:netlify",
    "ci:test": "npm run lint:strict && npm run type-check:strict && npm run test:ci",
    "ci:build": "npm run env:check && npm run content:full && npm run build",
    "ci:full": "npm run ci:test && npm run ci:build",
    "analyze": "cross-env ANALYZE=true NODE_OPTIONS=--max-old-space-size=4096 next build",
    "analyze:bundle": "npm run build -- --analyze",
    "analyze:bundles": "npx @next/bundle-analyzer",
    "analyze:lighthouse": "npx lighthouse http://localhost:3000 --output json --output-path ./reports/lighthouse.json",
    "docker:build": "docker build -t abraham-of-london .",
    "docker:run": "docker run -p 3000:3000 abraham-of-london",
    "docker:compose": "docker-compose up",
    "docker:compose:build": "docker-compose up --build",
    "docker:clean": "docker system prune -f",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "docs:generate": "npx typedoc --out docs src",
    "docs:serve": "npx serve docs",
    "docs:clean": "rimraf docs",
    "benchmark": "npx tsx scripts/system/benchmark.ts",
    "benchmark:api": "npx tsx scripts/system/benchmark-api.ts",
    "benchmark:pdf": "npx tsx scripts/system/benchmark-pdf-generation.ts",
    "security:scan": "npx tsx scripts/security/security-scan.ts",
    "security:audit": "pnpm audit --production --audit-level=high",
    "security:check": "npm run security:scan && npm run security:audit",
    "performance:test": "npx artillery run scripts/performance/performance-test.yml",
    "performance:profile": "NODE_ENV=production node --prof scripts/system/profile-performance.js",
    "profile:memory": "node --inspect scripts/system/profile-memory.js",
    "update:dependencies": "npx npm-check-updates -u",
    "update:dependencies:interactive": "npx npm-check-updates -i",
    "update:all": "npm run update:dependencies && pnpm install",
    "watch": "nodemon --watch src --ext ts,tsx --exec \"npm run dev\"",
    "watch:build": "nodemon --watch src --ext ts,tsx --exec \"npm run build\"",
    "netlify:dev": "cross-env NETLIFY=true NODE_ENV=development next dev",
    "netlify:build": "npm run build:netlify"
  },

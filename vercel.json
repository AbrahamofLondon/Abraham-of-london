{
  "version": 2,
  "buildCommand": "CI=false pnpm install && pnpm run build",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["lhr1"],
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "NEXT_DISABLE_TYPECHECK": "1",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}